{
  "name": "menstackjs",
  "version": "2.0.2",
  "description": "menstack",
  "scripts": {
    "dev": "node_modules/.bin/nodemon -w server/ --exec \"npm start\"",
    "start": "node ./run.js",
    "test": "npm run mocha && npm run lint",
    "mocha": "node ./tests/mocha.js mocha --exit",
    "lint": "node_modules/.bin/standard",
    "lint:fix": "node_modules/.bin/standard --fix",
    "postinstall": "node scripts/postinstall.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://GreenPioneer@github.com/greenpioneersolutions/menstackjs.git"
  },
  "keywords": [
    "menstackjs"
  ],
  "author": "GPS",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/greenpioneersolutions/menstackjs/issues"
  },
  "homepage": "https://github.com/greenpioneersolutions/menstackjs#readme",
  "dependencies": {
    "auto-parse": "1.5.1",
    "compression": "1.7.3",
    "connect-mongo": "2.0.1",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "dotenv": "6.0.0",
    "express": "4.16.3",
    "express-session": "1.15.6",
    "express-validator": "5.3.0",
    "glob": "7.1.3",
    "helmet": "3.13.0",
    "jsonwebtoken": "8.3.0",
    "lodash": "4.17.10",
    "method-override": "3.0.0",
    "mongoose": "5.2.12",
    "morgan": "1.9.0",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "run-auto": "2.0.3",
    "sendmail": "1.4.1",
    "serve-favicon": "2.5.0",
    "socket.io": "2.1.1",
    "spdy": "3.4.7",
    "winston": "3.0.0"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "mocha": "5.2.0",
    "nodemon": "1.18.4",
    "shelljs": "0.8.2",
    "standard": "12.0.1",
    "supertest": "3.1.0"
  },
  "standard": {
    "globals": [
      "describe",
      "expect",
      "it",
      "beforeEach",
      "before",
      "afterEach",
      "after"
    ],
    "ignore": [
      "logs/",
      "database/",
      "downloads/",
      "backups/",
      "tools/",
      "test/",
      "*.spec.js"
    ]
  }
}

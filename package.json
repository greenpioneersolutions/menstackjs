{
  "name": "menstackjs",
  "version": "2.0.0-alpha.3",
  "description": "menstack",
  "scripts": {
    "dev": "node_modules/.bin/nodemon -w server/src --exec \"npm start\"",
    "start": "npm run build && node ./server/dist/run.js",
    "test": "npm run mocha && npm run lint",
    "mocha": "npm run build && node ./server/dist/run-tests.js mocha  ",
    "build": "node_modules/.bin/babel server/src -s -D -d server/dist",
    "lint": "node_modules/.bin/standard",
    "lint:fix": "node_modules/.bin/standard --fix",
    "postinstall": "node scripts/postinstall.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://GreenPioneer@github.com/greenpioneersolutions/menstackjs.git"
  },
  "keywords": [
    "menstackjs"
  ],
  "author": "GPS",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/greenpioneersolutions/menstackjs/issues"
  },
  "homepage": "https://github.com/greenpioneersolutions/menstackjs#readme",
  "devDependencies": {
    "babel-cli": "^7.0.0-beta.3",
    "babel-core": "^7.0.0-beta.3",
    "babel-preset-es2015": "^7.0.0-beta.3",
    "babel-preset-stage-0": "^7.0.0-beta.3",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "mocha": "^5.0.5",
    "nodemon": "^1.17.2",
    "shelljs": "^0.8.1",
    "standard": "^11.0.1",
    "supertest": "^3.0.0"
  },
  "standard": {
    "globals": [
      "describe",
      "expect",
      "it",
      "beforeEach",
      "before",
      "afterEach",
      "after"
    ],
    "ignore": [
      "logs/",
      "database/",
      "downloads/",
      "backups/",
      "tools/",
      "test/",
      "*.spec.js"
    ]
  },
  "babel": {
    "retainLines": true,
    "presets": [
      "es2015",
      "stage-0"
    ],
    "ignore": [
      "server/src/**/test/*.js",
      "server/src/**/package.json",
      "server/src/**/node_modules/**.js"
    ]
  },
  "dependencies": {
    "dotenv": "^5.0.1",
    "glob": "^7.1.2"
  }
}

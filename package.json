{
  "name": "menstackjs",
  "version": "1.0.0",
  "description": "Do you need a full stack solution that is easy to build and all JavaScript? Then you're in the right place with Mean Stack JS. There is no steep learning curve and it only takes an average of 10 minutes to get your base code up and running. We've made the development process as simple and scalable as possible. This is perfect for anyone involved in software development, including User Experience Designers, Quality Assurance Engineers, Software Engineers, Frontend Developers, Backend Developers and Full Stack Engineers.",
  "main": "server.js",
  "standard": {
    "globals": [
      "angular",
      "toastr",
      "moment",
      "self",
      "_",
      "describe",
      "it",
      "__dirname",
      "localStorage",
      "io",
      "expect",
      "inject",
      "beforeEach",
      "before",
      "afterEach",
      "after"
    ],
    "ignore": [
      "commands/template/"
    ]
  },
  "scripts": {
    "test": "npm run mocha && npm run standard",
    "mocha": "node node_modules/mocha/bin/mocha tests/unit/mocha.test.js  ",
    "standard": "node node_modules/standard/bin/cmd.js || standard",
    "start": " node server.js",
    "development": "export NODE_ENV=development || set NODE_ENV=development && node node_modules/nodemon/bin/nodemon.js server.js || nodemon.js server.js",
    "qa": "export NODE_ENV=test || set NODE_ENV=test && node server.js",
    "production": "export NODE_ENV=production || set NODE_ENV=production && node server.js",
    "cli": "node commands/cli.js",
    "socketio": "node socketio.js"
  },
  "dependencies": {
    "auto-parse": "^1.0.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.15.2",
    "buildreq": "^2.1.0",
    "compression": "^1.6.2",
    "connect-mongo": "^1.3.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.0",
    "ejs": "^2.5.1",
    "express": "^4.14.0",
    "express-session": "^1.14.1",
    "express-validator": "^3.0.0",
    "helmet": "^3.1.0",
    "jsonwebtoken": "^7.1.9",
    "lodash": "^4.15.0",
    "method-override": "^2.3.6",
    "mongoose": "^4.5.10",
    "mongoose-timestamp": "^0.6.0",
    "mongoose-validator": "^1.2.5",
    "morgan": "^1.7.0",
    "multer": "^1.2.0",
    "nodemailer": "^2.5.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pug": "^2.0.0-beta6",
    "run-auto": "^2.0.0",
    "serial-concat-files": "^1.0.0",
    "socket.io": "^1.3.7",
    "swagger-node-express": "^2.1.3",
    "swagger-ui": "2.2.8"
  },
  "devDependencies": {
    "blessed": "^0.1.81",
    "chai": "^3.4.1",
    "glob": "^7.0.6",
    "inquirer": "^2.0.0",
    "mocha": "^3.0.2",
    "multiline": "^1.0.2",
    "nodemon": "^1.8.1",
    "phantomjs-prebuilt": "^2.1.7",
    "standard": "latest",
    "supertest": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/greenpioneersolutions/menstackjs.git"
  },
  "keywords": [
    "mean",
    "stack",
    "msjs",
    "mongo",
    "express",
    "angular",
    "node"
  ],
  "author": "Green Pioneer <info@greenpioneersolutions.com>",
  "contributors": "https://github.com/greenpioneersolutions/menstackjs/graphs/contributors",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/greenpioneersolutions/menstackjs/issues"
  },
  "homepage": "https://github.com/greenpioneersolutions/menstackjs#readme"
}
